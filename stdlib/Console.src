module std.io;
namespace Console;

import stdc.stdio;


public void Print(string str)
{
	stdio.printf("%s", str);
}

public void PrintLine(string str)
{
	stdio.printf("%s\n", str);
}

public void Error(string str)
{
	stdio.fprintf(stderr, "%s", str);
}

public void ErrorLine(string str)
{
	stdio.fprintf(stderr, "%s\n", str);
}
